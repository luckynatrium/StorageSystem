<table>
<tr>
  <th>Name</th>
  <th>Quantity</th>
  <th>Last delivery date</th>
</tr>

<% @stocks.each do |stock| %>                    
    <tr>
      <td><%= stock.name %></td>
      <td><%= stock.quantity %></td> 
      <td><%= stock.date.strftime("%d/%m/%Y") %></td>
    </tr>
  <% end %>
</table>
<%= link_to "Create Delivery", new_storage_delivery_path(storage_id: @storage_id), class: "my-button"%>
<%= link_to "All Deliveries", deliveries_path, class: "my-button"%>
<%= link_to "Create Transfer", transfers_choose_path, class: "my-button"%>
<%= link_to "All Transfers", transfers_path, class: "my-button"%>